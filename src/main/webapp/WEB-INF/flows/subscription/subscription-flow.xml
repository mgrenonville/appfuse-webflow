<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow
                          http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

    <view-state id="subscription" view="flow/subscription">
        <transition on="subscription" to="subscriptionP2" history="invalidate">

        </transition>
    </view-state>
    <view-state id="subscriptionP2" view="flow/subscription">
        <transition on="subscription" to="subscription" history="invalidate">

        </transition>
    </view-state>

</flow>